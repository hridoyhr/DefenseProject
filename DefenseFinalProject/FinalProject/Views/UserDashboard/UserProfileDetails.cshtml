@model UserProfileDetailsModel

@{
    ViewBag.Title = "Details";
}
<div class="body-bg-green register-form-div">
    @* Details Start *@
    <div id="site-wrapper" class="d-flex flex-column justify-content-center container ">

        <div id="breadbrumbs" class="d-flex justify-content-start container mt-4 mb-2 w-80" aria-label="breadcrumbs">
            <div class="btn-group container p-0" role="group">
                <a asp-controller="UserDashboard" asp-action="UserHome">
                    <button type="button" class="btn btn-sm btn-dark btn-arrow-right text-white">HOME</button>
                </a>

                <a asp-controller="UserDashboard" asp-action="UserProfile">
                    <button type="button" class="btn btn-sm btn-arrow-right btn-dark text-white">PROFILE DASH</button>
                </a>
                <a asp-controller="UserDashboard" asp-action="UserProfileDetails">
                    <button type="button" class="btn btn-sm btn-arrow-right btn-success text-white">DETAILS</button>
                </a>

            </div>
        </div>

        <div class="d-flex flex-column justify-content-center container w-80">
            <partial name="_UserDetails" model="Model" />
        </div>

    </div>

    @* Details End *@
</div>

